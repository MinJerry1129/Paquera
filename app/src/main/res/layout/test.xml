<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools" android:orientation="vertical"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <TextView
            android:text="My profile pics"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/profile_profile_pics2" android:editable="false"
            android:textSize="16dp"
            android:paddingBottom="6dp"
            android:textColor="@color/LightOrange"
            android:textStyle="bold" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <Button
            android:text="Edit"
            android:layout_width="wrap_content"
            android:id="@+id/profile_delete_account_button4"
            style="@style/Widget.AppCompat.Button.Borderless" android:textAllCaps="false"
            android:textColorHint="@color/CommonBlue" android:textColor="@color/CommonBlue"
            android:textSize="17dp" android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/profile_profile_pics2" android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
