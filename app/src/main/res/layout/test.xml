            android:text="Delete account"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/profile_delete_account_button5"
            style="@style/Widget.AppCompat.Button.Borderless" android:textAllCaps="false"
            android:textColorHint="@color/CommonBlue" android:textColor="@color/CommonBlue"
            android:textSize="17dp" android:textStyle="bold" tools:layout_editor_absoluteY="254dp"
            tools:layout_editor_absoluteX="0dp"/>
    <TextView
            android:text="Pics of my place"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/profile_profile_place2" android:editable="false"
            android:textSize="16dp"
            android:paddingBottom="6dp"
            android:textColor="@color/LightOrange"
            android:textStyle="bold" tools:layout_editor_absoluteX="195dp" tools:layout_editor_absoluteY="142dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>